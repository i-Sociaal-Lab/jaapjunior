{
	"name": "@jaap/frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"start": "bun run dist/index.html",
		"preview": "vite preview",
		"test:unit": "vitest",
		"typecheck": "vue-tsc --noEmit -p tsconfig.app.json",
