		"clsx": "^2.1.1",
		"dompurify": "^3.2.5",
		"hono": "4.9.7",
		"lucide-vue-next": "^0.542.0",
		"marked": "^16.0.0",
		"pinia": "^3.0.1",
		"reka-ui": "^2.2.0",
		"tailwind-merge": "^3.2.0",
		"tailwindcss": "^4.1.4",
		"tw-animate-css": "^1.2.8",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0"
