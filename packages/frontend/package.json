		"@vue/test-utils": "2.4.6",
		"@vue/tsconfig": "0.8.1",
		"eslint": "9.36.0",
		"eslint-plugin-oxlint": "1.17.0",
		"eslint-plugin-vue": "10.5.0",
		"jiti": "2.5.1",
		"jsdom": "27.0.0",
		"llamaindex": "0.11.28",
		"npm-run-all2": "8.0.4",
		"oxlint": "1.13.0",
		"prettier": "3.6.2",
		"typescript": "5.9.2",
		"vite": "7.1.5",
		"vite-plugin-vue-devtools": "8.0.1",
