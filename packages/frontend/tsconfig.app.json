{
	"extends": "@vue/tsconfig/tsconfig.dom.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue",
		"auto-imports.d.ts",
		"components.d.ts",
		"nuxt-ui.d.ts"
	],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"baseUrl": ".",
		"verbatimModuleSyntax": false,
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}
