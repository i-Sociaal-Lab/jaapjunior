{
	"name": "@jaap/api",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"scripts": {
		"dev": "bun --watch src/index.ts",
		"build": "tsc && bun build --compile --minify --sourcemap ./src/index.ts --outfile dist/api",
		"start": "./dist/api",
		"typecheck": "tsc --noEmit"
	},
	"dependencies": {
		"@hono/valibot-validator": "0.5.3",
		"@llamaindex/anthropic": "0.3.25",
		"@llamaindex/chroma": "0.0.36",
		"@llamaindex/google": "0.3.22",
		"@llamaindex/groq": "0.0.92",
		"@llamaindex/mistral": "0.1.22",
		"@llamaindex/openai": "0.4.20",
		"@llamaindex/readers": "3.1.21",
		"@valibot/to-json-schema": "1.3.0",
		"better-sqlite3": "12.2.0",
		"cohere-ai": "7.19.0",
		"hono": "4.11.10",
		"hono-openapi": "1.0.8",
		"jose": "6.1.0",
		"llamaindex": "0.12.0",
		"valibot": "1.1.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "7.6.13",
		"typescript": "5.9.2"
	}
}
