<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://www.istandaarden.nl/iwmo/3_2/basisschema/schema" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:iwmo="http://www.istandaarden.nl/iwmo/3_2/basisschema/schema">
	<xs:annotation>
		<xs:appinfo>
			<iwmo:standaard>iwmo</iwmo:standaard>
			<iwmo:bericht>basisschema</iwmo:bericht>
			<iwmo:release>3.2</iwmo:release>
			<iwmo:BasisschemaXsdVersie>1.0.0</iwmo:BasisschemaXsdVersie>
			<iwmo:BasisschemaXsdMinVersie>1.0.0</iwmo:BasisschemaXsdMinVersie>
			<iwmo:BasisschemaXsdMaxVersie>1.0.0</iwmo:BasisschemaXsdMaxVersie>
		</xs:appinfo>
	</xs:annotation>
	<xs:complexType name="CDT_Achternaam">
		<xs:annotation>
			<xs:documentation>De achternaam van een natuurlijk persoon, aangeduid als Naam met Voorvoegsel.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Achternaam" type="iwmo:LDT_Naam">
				<xs:annotation>
					<xs:documentation>De achternaam van een persoon, indien nodig verkort weergegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorvoegsel" type="iwmo:LDT_Voorvoegsel" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Verzameling van een of meer voorzetsels/lidwoorden, die aan het significante deel van de achternaam van een persoon vooraf gaat en daar een onderdeel van is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Adres">
		<xs:annotation>
			<xs:documentation>Gegevens voor de aanduiding van een adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huis" type="iwmo:CDT_Huis" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Postcode" type="iwmo:LDT_Postcode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Aanduiding van de (inter)nationale postcode. Dit veld is geschikt voor zowel de Nederlandse als niet-Nederlandse postcodes.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Straatnaam" type="iwmo:LDT_Straatnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De officiele door de gemeente vastgestelde naam van een straat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Plaatsnaam" type="iwmo:LDT_Plaatsnaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De door de gemeente vastgestelde naam van een woonplaats.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="LandCode" type="iwmo:LDT_LandCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Identificatie van een (historisch/actueel) land of gebiedsdeel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BedragMetDC">
		<xs:annotation>
			<xs:documentation>Samengesteld bedrag met DebetCredit indicatie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Bedrag" type="iwmo:LDT_Bedrag">
			</xs:element>
			<xs:element name="DebetCredit" type="iwmo:LDT_DebetCredit">
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_BerichtIdentificatie">
		<xs:annotation>
			<xs:documentation>Naam of nummer en dagtekening ter identificatie van een totale aanlevering.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Identificatie" type="iwmo:LDT_IdentificatieBericht">
				<xs:annotation>
					<xs:documentation>Naam of nummer, die ter identificatie aan een totale aanlevering kan worden meegegeven.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Dagtekening" type="iwmo:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De datum van verzending van het bericht.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Communicatie">
		<xs:annotation>
			<xs:documentation>Aanduiding over bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Vorm" type="iwmo:LDT_Communicatievorm">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van de bijzondere vorm van communicatie die gebruikt dient te worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Taal" type="iwmo:LDT_Taal" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De taal van de client waarin met de client wordt gecommuniceerd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Geboortedatum">
		<xs:annotation>
			<xs:documentation>Datum waarop een natuurlijk persoon geboren is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Datum" type="iwmo:LDT_Datum">
				<xs:annotation>
					<xs:documentation>Datum waarop een persoon geboren is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DatumGebruik" type="iwmo:LDT_DatumGebruik" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicator die aangeeft welk deel van de voorafgaande datum gebruikt mag worden.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_GeslotenPeriode">
		<xs:annotation>
			<xs:documentation>Tijdsperiode met zowel begin- als einddatum</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="iwmo:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="iwmo:LDT_Datum">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Huis">
		<xs:annotation>
			<xs:documentation>Aanduiding van een specifiek huis in een straat, onderdeel van het adres.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Huisnummer" type="iwmo:LDT_Huisnummer">
				<xs:annotation>
					<xs:documentation>De numerieke aanduiding zoals de gemeente die aan het object heeft toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Huisletter" type="iwmo:LDT_Huisletter" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Een alfabetisch teken achter het huisnummer zoals dit door het gemeentebestuur is toegekend.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="HuisnummerToevoeging" type="iwmo:LDT_HuisnummerToevoeging" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Die letters of tekens die noodzakelijk zijn om, naast huisnummer en -letter, de brievenbus te vinden.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AanduidingWoonadres" type="iwmo:LDT_AanduidingWoonadres" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De aanduiding die wordt gebruikt voor adressen die niet zijn voorzien van de gebruikelijke straatnaam en huisnummeraanduidingen.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Omvang">
		<xs:annotation>
			<xs:documentation>Aanduiding van de omvang van de te leveren of geleverde ondersteuning, uitgedrukt in Volume, Eenheid en Frequentie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Volume" type="iwmo:LDT_Volume">
				<xs:annotation>
					<xs:documentation>Aanduiding van de mate van zorg betreffende een product.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Eenheid" type="iwmo:LDT_Eenheid">
				<xs:annotation>
					<xs:documentation>Aanduiding van de eenheid waarin het volume is uitgedrukt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Frequentie" type="iwmo:LDT_Frequentie">
				<xs:annotation>
					<xs:documentation>Aanduiding van de frequentie waarmee een ondersteuningsproduct geleverd wordt.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_OpenPeriode">
		<xs:annotation>
			<xs:documentation>Aanduiding van een tijdsperiode met een Begindatum en/of een Einddatum.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Begindatum" type="iwmo:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De begindatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Einddatum" type="iwmo:LDT_Datum" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De einddatum van de periode.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Product">
		<xs:annotation>
			<xs:documentation>Gecodeerde omschrijving van een product, dienst of resultaat ten behoeve van het leveren van ondersteuning aan een client. Product wordt uitgedrukt in een categorie en eventueel nader gespecificeerd met een specifieke productcode.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Categorie" type="iwmo:LDT_ProductCategorie">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van hoofdcategorieen van producten in het kader van de Wet maatschappelijke ondersteuning (Wmo).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Code" type="iwmo:LDT_ProductCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van producten in het kader van de Wet maatschappelijke ondersteuning (Wmo).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Referentie">
		<xs:sequence>
			<xs:element name="ReferentieNummer" type="iwmo:LDT_ReferentieNummer">
				<xs:annotation>
					<xs:documentation>Uniek referentienummer dat de zorgverlener heeft toegekend aan een detailregel van de declaratie.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VorigReferentieNummer" type="iwmo:LDT_ReferentieNummer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Uniek referentienummer dat de zorgverlener heeft toegekend aan een voorgaande detailregel, gerelateerd aan deze detailregel.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoon">
		<xs:annotation>
			<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoonnummer" type="iwmo:LDT_Telefoonnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Landnummer" type="iwmo:LDT_Landnummer">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer van een land, vanuit Nederland benaderd.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Telefoonnummers">
		<xs:annotation>
			<xs:documentation>De telefoonnummers waarop de client of relatie van de client te bereiken is.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Telefoon01" type="iwmo:CDT_Telefoon">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Telefoon02" type="iwmo:CDT_Telefoon" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Het telefoonnummer waarop de client of relatie van de client te bereiken is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_TotaalBedragMetDC">
		<xs:annotation>
			<xs:documentation>Samengesteld totaal bedrag met DebetCredit indicatie.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TotaalBedrag" type="iwmo:LDT_TotaalBedrag">
				<xs:annotation>
					<xs:documentation>Het bedrag dat in zijn totaliteit gedeclareerd of gefactureerd, toegekend of berekend is.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DebetCredit" type="iwmo:LDT_DebetCredit">
				<xs:annotation>
					<xs:documentation>Indicatie of een bedrag/aantal debet of credit is.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_VerkorteNaam">
		<xs:annotation>
			<xs:documentation>De naam van een natuurlijk persoon, aangeduid als Geslachtsnaam en eventueel Voornamen en/of Voorletters.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geslachtsnaam" type="iwmo:CDT_Achternaam">
				<xs:annotation>
					<xs:documentation>Wettelijk vastgelegde achternaam die iemand bij de geboorte erft van de vader of moeder.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voornamen" type="iwmo:LDT_Naam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling namen die, gescheiden door spaties, aan de geslachtsnaam voorafgaat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorletters" type="iwmo:LDT_Voorletters" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling van letters die wordt gevormd door de eerste letter van alle in volgorde voorkomende voornamen van een persoon.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_Verwijzer">
		<xs:annotation>
			<xs:documentation>Gegevens over de persoon of instantie die een client heeft doorverwezen naar ondersteuning, aangeduid als Type en Naam of ZorgverlenerCode.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Type" type="iwmo:LDT_TypeVerwijzer">
				<xs:annotation>
					<xs:documentation>Gecodeerde aanduiding van het type verwijzer.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Naam" type="iwmo:LDT_NaamVerwijzer" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Naam van de verwijzende persoon of instantie.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ZorgverlenerCode" type="iwmo:LDT_AgbCode" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De AGBcode van de zorgverlener.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_VolledigeNaam">
		<xs:annotation>
			<xs:documentation>Volledige naam van een natuurlijk persoon, aangeduid als Geslachtsnaam, eventueel Partnernaam, Voornamen en/of Voorletters en NaamGebruik.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Geslachtsnaam" type="iwmo:CDT_Achternaam">
				<xs:annotation>
					<xs:documentation>Wettelijk vastgelegde achternaam die iemand bij de geboorte erft van de vader of moeder.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Partnernaam" type="iwmo:CDT_Achternaam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Na het voltrekken van een huwelijk of na een officiele partnerregistratie van de partner aangenomen familienaam.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voornamen" type="iwmo:LDT_Naam" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling namen die, gescheiden door spaties, aan de geslachtsnaam voorafgaat.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Voorletters" type="iwmo:LDT_Voorletters" minOccurs="0">
				<xs:annotation>
					<xs:documentation>De verzameling van letters die wordt gevormd door de eerste letter van alle in volgorde voorkomende voornamen van een persoon.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NaamGebruik" type="iwmo:LDT_NaamGebruik">
				<xs:annotation>
					<xs:documentation>Aanduiding naamgebruik (gecodeerd).</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CDT_XsdVersie">
		<xs:sequence>
			<xs:element name="BasisschemaXsdVersie" type="iwmo:LDT_Versie">
			</xs:element>
			<xs:element name="BerichtXsdVersie" type="iwmo:LDT_Versie">
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="LDT_AanduidingWoonadres">
		<xs:annotation>
			<xs:documentation>De aanduiding die wordt gebruikt voor adressen die niet zijn voorzien van de gebruikelijke straatnaam en huisnummeraanduidingen.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="AB">
				<xs:annotation>
					<xs:documentation>Aan boord</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="BY">
				<xs:annotation>
					<xs:documentation>Bij</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TO">
				<xs:annotation>
					<xs:documentation>Tegenover</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="WW">
				<xs:annotation>
					<xs:documentation>Woonwagen</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_AdresSoort">
		<xs:annotation>
			<xs:documentation>Nadere typering van het adres.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="01">
				<xs:annotation>
					<xs:documentation>BRP-adres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="02">
				<xs:annotation>
					<xs:documentation>Correspondentie-adres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="03">
				<xs:annotation>
					<xs:documentation>Verblijfadres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="04">
				<xs:annotation>
					<xs:documentation>Tijdelijk verblijfadres</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:maxLength value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_AgbCode">
		<xs:annotation>
			<xs:documentation>Identificerende code van een instelling of aanbieder van zorg of ondersteuning (zie https://www.agbcode.nl)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="[0-9]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_Bedrag">
		<xs:annotation>
			<xs:documentation>Bedrag in 1/100 van gehanteerde valuta. Bijvoorbeeld 10000 is tienduizend eurocent, is 100 Euro.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99999999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BerichtCode">
		<xs:annotation>
			<xs:documentation>Code ter identificatie van een soort bericht. Per bericht is gespecificeerd welke code gebruikt moet worden. De volledige codelijst is te vinden op het portaal van Vektis (http://ei.vektis.nl/WespCodelijstenDetail.aspx?Co_Ge_Code=COD002&amp;Co_Or_Code=VEKT).</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BerichtSubversie">
		<xs:annotation>
			<xs:documentation>Volgnummer van de formele uitgifte van een minor release van een iStandaard.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_BerichtVersie">
		<xs:annotation>
			<xs:documentation>Volgnummer van de formele uitgifte van een major release van een iStandaard.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="99"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="LDT_Budget">
		<xs:annotation>
			<xs:documentation>Budget in 1/100 van gehanteerde valuta. Bijvoorbeeld 10000 is tienduizend eurocent, is 100 Euro.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
